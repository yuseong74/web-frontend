{
  "configurations": [
    {
      "name": "Web(chrome) 브라우저 디버깅",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "url": "http://localhost:6006",
      "webRoot": "${workspaceFolder}",
      "request": "launch",
      "type": "chrome"
    },
    {
      "name": "Node.js 런타임 디버깅",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "request": "launch",
      "type": "node"
    }
  ]
}
